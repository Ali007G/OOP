#include<iostream>
using namespace std;
class Distance {
private:
    int feet,inches;
public:
    Distance()
    {
        feet=0;
        inches=0;
    }
    Distance(int f,int i)
    {
    feet=f;
    inches=i;
    }
    void displaydistance()
    {
        cout<<"F : "<<feet<<"\nI : "<< inches<<endl;
    }
    Distance operator-()
    {
    feet=-feet;
    inches=-inches;
    return Distance(feet,inches);
    }
};
int main()
{
    Distance d1(11,10),d2(-5,11);
    -d1;
    d1.displaydistance();

    -d2;
    d2.displaydistance();
    return 0;
}
