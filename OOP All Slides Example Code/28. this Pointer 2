#include<iostream>
using namespace std;

class demo {
public:
    int num=20;
    void show(int num)
    {
        cout<<"Demo address "<<this<<endl;
        cout<<"Object value "<<this->num<<endl;
        cout<<"Function value "<<num<<endl;
    }
};
int main()
{
    demo ob1,ob2;
    ob1.show(30);
    ob2.show(50);
    return 0;
}
